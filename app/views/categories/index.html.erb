All Jobs for <strong><%= @company.name %></strong>

<ul>
  <% @jobs.each do |job| %>
  <li class=job_<%= job.id %>>
  <%= job.title %> at <%= job.company.name %>
  <%= link_to "Edit", edit_company_job_path(params[:company_id], job.id) %>
  <%= link_to "Delete", company_job_path(params[:company_id], job.id), method: "delete" %>
  <% end %>
</ul>
