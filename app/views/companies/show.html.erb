<div class="show">
  <div class="title">
    <h2> Contacts for <strong><%= @company.name %></strong> </h2>
  </div>

  <div class="company-container container-fluid">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
          <%= render partial: 'companies/contact', collection: @company.contacts %>
        </h3>
      </div>
      <div class="panel-body">
          <%= render partial: 'contacts/form' %>
      </div>
    </div>
  </div>
</div>
