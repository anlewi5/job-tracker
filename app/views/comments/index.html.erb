All Comments for <strong><%= @job.title %></strong>

<ul>
  <% @comments.each do |comment| %>
  <li class=comment_<%= comment.id %>>
  <%= comment.title %> at <%= comment.job.title %>
  <%= link_to "Edit", edit_job_comment_path(params[:job_id], comment.id) %>
  <%= link_to "Delete", job_comment_path(params[:job_id], comment.id), method: "delete" %>
  <% end %>
</ul>
